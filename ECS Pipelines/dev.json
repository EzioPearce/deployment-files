{
	"family": "flam-zingcam-dev",
	"taskRoleArn": "arn:aws:iam::214493502073:role/FlamServicesTaskRole",
	"executionRoleArn": "arn:aws:iam::214493502073:role/FlamServicesTaskRole",
	"networkMode": "bridge",
	"containerDefinitions": [{
		"name": "flam-zingcam-dev",
		"image": "${IMAGE}",
		"cpu": 2048,
		"memory": 12288,
		"memoryReservation": 12288,
		"portMappings": [{
			"containerPort": 3000,
			"hostPort": 0,
			"protocol": "tcp"
		}],
		"essential": true,
		"logConfiguration": {
			"logDriver": "awslogs",
			"secretOptions": null,
			"options": {
				"awslogs-group": "/ecs/flam-zingcam-dev",
				"awslogs-region": "ap-south-1",
				"awslogs-stream-prefix": "ecs"
			}
		},
		"environment": [{
				"name": "APP_ENVIROMENT",
				"value": "dev"
			},
			{
				"name": "CONTEXT",
				"value": "zingcam"
			},
			{
				"name": "APP_PORT",
				"value": "3000"
			},
			{
				"name": "AWS_REGION",
				"value": "ap-south-1"
			},
			{
				"name": "REGION_NAME",
				"value": "ap-south-1"
			}
		],
		"resourceRequirements": [{
			"type": "GPU",
			"value": "1"
		}],
		"secrets": [{
				"name": "AWS_ACCESS_KEY_ID",
				"valueFrom": "/conf/zingcam/dev/AWS_ACCESS_KEY_ID"
			},
			{
				"name": "ZINGCAM_ADMIN_SECRET_KEY",
				"valueFrom": "/conf/zingcam/dev/ZINGCAM_ADMIN_SECRET_KEY"
			},
			{
				"name": "AWS_SECRET_ACCESS_KEY_ID",
				"valueFrom": "/conf/zingcam/dev/AWS_SECRET_ACCESS_KEY_ID"
			},
			{
				"name": "DATABASE_MONGO_URL",
				"valueFrom": "/conf/zingcam/dev/DATABASE_MONGO_URL"
			},
			{
				"name": "ASPECTO_AUTH",
				"valueFrom": "/conf/flam-aspecto/dev/ASPECTO_AUTH"
			},
			{
				"name": "LAMBDA_URL",
				"valueFrom": "/conf/zingcam/dev/LAMBDA_URL"
			},
			{
				"name": "AWS_BUCKET_NAME",
				"valueFrom": "/conf/zingcam/dev/AWS_BUCKET_NAME"
			},
			{
				"name": "AWS_FILE_PREFIX",
				"valueFrom": "/conf/zingcam/dev/AWS_FILE_PREFIX"
			},
			{
				"name": "BUCKET_NAME",
				"valueFrom": "/conf/zingcam/dev/BUCKET_NAME"
			},
			{
				"name": "FILE_PREFIX",
				"valueFrom": "/conf/zingcam/dev/FILE_PREFIX"
			},
		],
		"hostname": "zingcam",
		"dockerLabels": {
			"Environment": "dev",
			"Name": "zingcam",
			"Version": "${VERSION}"
		},
		"healthCheck": {
			"command": [
				"CMD-SHELL",
				"curl --fail http://localhost:3000/zingcam/health || exit 1"
			],
			"interval": 5,
			"timeout": 4,
			"retries": 3,
			"startPeriod": 5
		}
	}],
	"requiresCompatibilities": ["EC2"],
	"cpu": "2048",
	"memory": "12288",
	"tags": [{
			"key": "GITHUB_BRANCH",
			"value": "${GITHUB_REF}"
		},
		{
			"key": "GITHUB_BUILD_NUMBER",
			"value": "${GITHUB_RUN_NUMBER}"
		},
		{
			"key": "GITHUB_COMMIT",
			"value": "${GITHUB_SHA}"
		},
		{
			"key": "GITHUB_REPO_URL",
			"value": "https://github.com/homingos/flam-services-zingcam"
		},
		{
			"key": "Version",
			"value": "${VERSION}"
		},
		{
			"key": "Environment",
			"value": "dev"
		},
		{
			"key": "Name",
			"value": "ZINGCAM"
		}
	]
}
